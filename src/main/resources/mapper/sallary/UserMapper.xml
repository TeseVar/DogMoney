<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace为命名空间，应该是mapper接口的全称-->
<mapper namespace="com.meng.springboot.sallary.mapper.UserMapper">
    <select id="getUserList" resultType="com.meng.springboot.sallary.entity.User">
        select * from user;
    </select>

</mapper>